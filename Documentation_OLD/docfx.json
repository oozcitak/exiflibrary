{
  "metadata": [
    {
      "src": [
        {
          "files": "**.csproj",
          "src": "../ExifLibrary"
        }
      ],
      "filter": "filterConfig.yml",
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [ "index.md", "combinedtoc/**", "toc.yml", "articles/**.yml", "articles/**.md", "obj/api/**.yml" ],
    "resource": [ "resources/icons/**" ],
    "overwrite": [ "overwrites/**.md" ],
    "globalMetadata": {
      "_appTitle": "ExifLibrary",
      "_appFooter": "Copyright © 2019 Özgür Özçıtak",
      "_appLogoPath": "resources/icons/logo.png",
      "_appFaviconPath": "resources/icons/favicon.ico",
      "_disableBreadcrumb": true,
      "_disableAffix": true
    },
    "dest": "_site",
    "template": [ "default" ]
  }
}